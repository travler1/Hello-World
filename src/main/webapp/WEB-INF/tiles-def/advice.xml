		<put-attribute name="title" value="보낸 첨삭요청함" />
		<put-attribute name="body" value="/WEB-INF/views/matching/advice/sentAdvice.jsp"/>
	</definition>
	<!-- 특정 첨삭 출력 -->
	<definition name="AdviceDetail" extends="myPage">
		<put-attribute name="title" value="첨삭확인" />
